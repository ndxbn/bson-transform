{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:js-lib",
    ":disableDependencyDashboard",
    ":automergeMinor",
    ":rebaseStalePrs",
    ":respectLatest",
    ":prHourlyLimitNone",
    ":prConcurrentLimitNone",
    "group:linters",
    "group:allNonMajor"
  ],
  "lockFileMaintenance": {
    "enabled": true
  }
}
